{
	"name": "@superset/stream-server",
	"version": "0.0.1",
	"description": "Durable Stream server for real-time token streaming",
	"type": "module",
	"main": "./dist/index.js",
	"scripts": {
		"dev": "tsx watch src/index.ts",
